{
  "name": "template-extension-migration",
  "displayName": "template-extension-migration",
  "version": "1.0.0",
  "author": "Jason Garner",
  "description": "Simple extension to make template responses for professional emails/texts with rich text capability for fancy formatting, powered by SunEditor.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "vanilla",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{js,json,css,scss,md}'",
    "zip": "npm run build && node src/zip.js"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.19",
    "gulp": "^4.0.2",
    "gulp-zip": "^6.0.0",
    "prettier": "^3.0.3",
    "vite": "^4.4.11"
  },
  "dependencies": {
    "html-to-text": "^9.0.5",
    "suneditor": "^2.46.2"
  }
}
